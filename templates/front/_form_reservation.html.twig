{{ form_start(form) }}


        <div class="container-participants" data-prototype="{{ form_widget(form.participants.vars.prototype)|e('html_attr') }}">

        {% for row in form.participants %}
            <div class="card-participant">
                {{ form_widget(row) }}
            </div>
        {% endfor %}
    </div>
    <div class="d-flex justify-content-center">
        <button class="btn btn-warning text-light rounded-pill  mb-3">{{ button_label|default('Enregistrer') }}</button>
    </div>

{{ form_end(form) }}
